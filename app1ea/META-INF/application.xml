<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/javaee"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/application_6.xsd"
             version="6">
    <application-name>app1</application-name>
    <description>Java EE Test Application</description>
    <display-name>app1 The Java EE Application Template</display-name>
    <initialize-in-order>true</initialize-in-order>
    <module id="app1ea-ejb">
        <ejb>app1ea-ejb.jar</ejb>
    </module>
    <module id="app1ea-web">
        <web>
            <web-uri>app1ea-web.war</web-uri>
            <context-root>app1ea-web</context-root>
        </web>
    </module>
    <module id="app1ea-webservice">
        <web>
            <web-uri>app1ea-webservice.war</web-uri>
            <context-root>app1ea-webservice</context-root>
        </web>
    </module>
    <security-role>
        <description>Administrator</description>
        <role-name>admin</role-name>
    </security-role>
    <security-role>
        <description>User</description>
        <role-name>user</role-name>
    </security-role>
    <library-directory>lib</library-directory>
</application>
